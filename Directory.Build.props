<Project ToolsVersion="15.0">
  <PropertyGroup>
    <TargetFramework>net471</TargetFramework>
    <NetStandardImplicitPackageVersion Condition=" '$(TargetFramework)' == 'netstandard2.0' ">2.0.0</NetStandardImplicitPackageVersion>
  </PropertyGroup>
  <PropertyGroup Condition="'$(TargetFramework)' == 'net471'">
    <TargetFrameworkIdentifier>.NETFramework</TargetFrameworkIdentifier>
    <TargetFrameworkVersion>v4.7.1</TargetFrameworkVersion>
  </PropertyGroup>
  <PropertyGroup>
    <PlatformTarget Condition=" '$(PlatformTarget)' == '' ">x64</PlatformTarget>
    <Configuration Condition=" '$(Configuration)' == '' ">Debug</Configuration>
  </PropertyGroup>
  <PropertyGroup>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <PropertyGroup>
    <BuildDir Condition=" '$(BuildDir)' == '' And '$(OS)' != 'Unix' ">$(SolutionDir)</BuildDir>
    <BuildDir Condition=" '$(BuildDir)' == '' And '$(OS)' == 'Unix' ">$(SolutionDir)build/</BuildDir>
    <OutDir Condition="'$(OS)' != 'Unix'">$(BuildDir)/bin/$(Configuration)/</OutDir>
    <OutDir Condition="'$(OS)' == 'Unix'">$(BuildDir)/bin/</OutDir>
    <BaseIntermediateOutputPath>$(BuildDir)obj/$(Configuration)/$(ProjectName)/</BaseIntermediateOutputPath>
    <IntermediateOutputPath>$(BaseIntermediateOutputPath)</IntermediateOutputPath>
    <OutputPath>$(BuildDir)/bin/</OutputPath>
    <BaseOutputPath>$(OutputPath)</BaseOutputPath>
    <MSBuildProjectExtensionsPath>$(BaseIntermediateOutputPath)</MSBuildProjectExtensionsPath>
  </PropertyGroup>
</Project>
