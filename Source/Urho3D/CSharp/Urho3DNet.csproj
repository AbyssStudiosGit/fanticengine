<Project Sdk="Microsoft.NET.Sdk" ToolsVersion="15.0">
  <PropertyGroup>
    <AssemblyName>Urho3DNet</AssemblyName>
    <OutputType>Library</OutputType>
  </PropertyGroup>
  <ItemGroup>
    <Compile Include="$(BuildDir)Source/Urho3D/CSharp/*.cs" />
  </ItemGroup>
  <Target Name="TestMessage" AfterTargets="Build" >
    <!--Use $(Property Name) to reference a property-->
    <Message Text="NETStandardImplicitPackageVersion = '$(NETStandardImplicitPackageVersion)'" Importance="high"/>
  </Target>
</Project>
